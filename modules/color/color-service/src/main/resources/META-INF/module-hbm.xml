<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.payment.color.model.Color" />
	<class name="com.payment.color.model.impl.ColorImpl" table="COLORS_Color">
		<id access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="colorId" type="long">
			<generator class="assigned" />
		</id>
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" column="uuid_" name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>